    "deploy": "gh-pages -d example/build",
    "lint": "eslint -c .eslintrc.json --ext .ts src/"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@testing-library/jest-dom": "^5.11.9",
